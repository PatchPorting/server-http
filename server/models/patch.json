{
  "name": "patch",
  "description": "Patch candidates our system found automatically. All include a field to show if they can be applied automatically.",
  "base": "PersistedModel",
  "options": {
    "validateUpsert": true
  },
  "forceId": false,
  "properties": {
    "link": {
      "type": "string",
      "required": true,
      "description": "Original link in which the system found the candidate"
    },
    "url": {
      "type": "string",
      "required": true,
      "description": "URL generated by the system to download the clean patch code"
    }
  },
  "validations": [],
  "relations": {
    "hunks": {
      "type": "hasMany",
      "model": "hunk",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
